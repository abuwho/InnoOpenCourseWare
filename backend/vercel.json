{
    "version": 2,
    "builds": [
      { "src": "/backend", "use": "@vercel/python" }
    ],
    "routes": [
      {
        "src": "/staticfiles/(.*)",
        "dest": "staticfiles/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/backend/$1",
        "headers": {
          "Cache-Control": "max-age=0"
        }
      }
    ]
  }
  