{
    "version": 2,
    "builds": [
      { "src": "/backend", "use": "@vercel/python", "config": { "maxLambdaSize": "15mb" }, "env": { "VERCEL_DEV_ENTRYPOINT": "python3 manage.py makemigrations && python3 manage.py migrate &&  python3 manage.py runserver" }}
    ],
    "routes": [
      {
        "src": "/staticfiles/(.*)",
        "dest": "staticfiles/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/backend/$1",
        "headers": {
          "Cache-Control": "max-age=0"
        }
      }
    ]
  }
  